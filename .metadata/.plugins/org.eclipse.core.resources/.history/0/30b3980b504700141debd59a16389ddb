	public void run() {
		while(true) {
			try {
				Thread.sleep(1000); //1초마다
			} catch ( InterruptedException e) {}
			
			currentTime++;
			if(currentTime)
		}
		
	}
	
	public void fm() {
		clientQueue.initialize();
		
	}

}
