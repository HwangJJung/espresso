
public class Client implements Observer {
	
	
	int arrivalTime;
	int ticketingTime;
	Station startStation;
	Station arrivalStation;
	Ticket ticket;
	Boolean isHadTicket;
	
	public Client() {}
	
	
	@Override
	public void update(ReleaseObservable clerk) {
		System.out.println("Client : " + clerk + "is Release the ticket.");
		
	}
}
