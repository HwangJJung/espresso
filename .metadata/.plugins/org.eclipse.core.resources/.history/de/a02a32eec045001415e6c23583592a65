
public class Client implements Observer {
	
	
	int arrivalTime;
	int ticketingTime;
	Station startStation;
	Station arrivalStation;
	Ticket ticket;
	Boolean isHadTicket;
	
	public Client() {}
	
	
	@Override
	public void update(ReleaseObservable client) {
		System.out.println("Clerk: " + client + "is comming");
		
	}

}
