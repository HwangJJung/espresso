package command;


public class Ticket {
	
	String name;
	Station startStation;
	Station arrivalStation;
	Price price;
	
	
	public void release() {
		System.out.println("Ticket Release. Have a Good Trip.");
	}
	
	public String getName() {
		return name;
	}
	
	public String toString() {
		return name;
		
	}
	public void setName(String name) {
		this.name = name;
	}

}
