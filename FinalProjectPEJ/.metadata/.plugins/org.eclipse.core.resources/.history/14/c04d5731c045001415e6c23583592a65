
public class Client2 implements Observer {
	
	Station startStation;
	int id;
	Command slot;
	
	public Client2() {}
	
	public void setCommand(Command command) {
		slot = command;
	}
	
	@Override
	public void update(OrderObservable client) {
		System.out.println("Clerk: " + client + "is comming");
		slot.execute();
	}

}
