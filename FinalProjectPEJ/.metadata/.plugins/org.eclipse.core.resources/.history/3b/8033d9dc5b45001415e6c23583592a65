package Factory;



public class Ticket {
	
	String name;
	Point  startPoint;
	Point  arrivalPoint;
	Price price;
	
	public Ticket(TicketComponentFactory)
	
	
	public void release() {
		System.out.println("Ticket Release. Have a Good Trip.");
	}
	
	public String getName() {
		return name;
	}
	
	public String toString() {
		return name;
		
	}
	public void setName(String name) {
		this.name = name;
	}

}
