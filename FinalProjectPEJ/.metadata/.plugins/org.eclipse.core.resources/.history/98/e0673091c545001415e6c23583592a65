
public class TicketGenerator extends Observable{
	
	Ticket ticket;
	
	@Override
	public Ticket getTicket() {
		
		return ticket;
	}

	@Override
	public void execute(Client client) {
		// TODO Auto-generated method stub
		ticket.name = client.name;
		ticket.startStation = client.startStaion;
		ticket.arrivalStation = client.arrivalStation;
		notifyObservers();
		
	}

}
